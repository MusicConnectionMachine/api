{
  "name": "MusicConnectsMachineAPI",
  "version": "0.0.1",
  "private": true,
  "description": "New Swagger API Project",
  "keywords": [],
  "author": "",
  "license": "",
  "main": "app.js",
  "dependencies": {
    "chai": "^3.5.0",
    "express": "^4.12.3",
    "mocha": "^3.2.0",
    "moment": "^2.18.1",
    "pg": "6.1.4",
    "pg-hstore": "2.3.2",
    "sequelize": "3.30.2",
    "swagger-express-mw": "^0.1.0"
  },
  "devDependencies": {
    "eslint": "^3.18.0",
    "npm-run-all": "^4.0.2",
    "should": "^7.1.0",
    "supertest": "^1.0.0"
  },
  "scripts": {
    "postinstall": "npm install -g multi-file-swagger",
    "swagger-compile": "cd ./api/swagger/src/ && multi-file-swagger ./swagger.yaml > ../swagger.json && cd ../../../",
    "start": "node app.js",
    "prestart": "npm run swagger-compile",
    "pretest": "npm run swagger-compile",
    "test": "npm-run-all test:* lint",
    "test:mocha": "mocha \"mocha\"",
    "test:swagger": "swagger_swagger_fileName=api/swagger/swagger.json swagger project test",
    "lint": "./node_modules/.bin/eslint ."
  }
}
